{
  "name": "Delete Backups",
  "version": "0.0.1",
  "slug": "delete_backups",
  "description": "Delete local backups on HASOS, keeping selected number",
  "url": "https://github.com/dbhardman/hassio-addons",
  "startup": "once",
  "boot": "manual",
  "advanced": true,
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "hassio_api": true,
  "hassio_role": "backup",
   "map": [
    "backup:rw",
  ],
  "options": {
    "max_local_backups": 4,
  },
  "schema": {
    "max_local_backups": "int",

  }
}
